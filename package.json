{
  "name": "flexboard",
  "version": "1.0.0",
  "description": "Flexport news board",
  "repository": "git@github.com:ZixiaoWang/flexboard.git",
  "author": "Jacky Wang <zwang@flexport.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@parcel/packager-raw-url": "2.4.1",
    "@parcel/transformer-sass": "2.4.1",
    "@parcel/transformer-webmanifest": "2.4.1",
    "gh-pages": "^3.2.3",
    "parcel": "^2.4.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "history": "^5.3.0",
    "preact": "^10.7.1",
    "preact-router": "^4.0.1"
  },
  "scripts": {
    "build": "parcel build --no-cache --no-source-maps --dist-dir=dist src/index.html",
    "serve": "parcel serve --no-cache --no-source-maps src/index.html",
    "watch": "parcel watch --no-source-maps src/index.html",
    "deploy": "rm -rf ./dist && yarn build && gh-pages --dist dist"
  }
}
